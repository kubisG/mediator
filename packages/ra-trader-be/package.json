        "start:linux": "cross-env NODE_ENV=dev tsc-watch --onSuccess \"node dist/main.js\"",
        "start:play": "cross-env NODE_ENV=dev nodemon --config nodemon-play.json",
        "start:hub": "cross-env NODE_ENV=dev nodemon --config nodemon-hub.json",
