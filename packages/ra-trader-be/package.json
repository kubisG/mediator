        "test": "cross-env NODE_ENV=test jest --verbose --passWithNoTests",
        "test:one": "cross-env NODE_ENV=test jest --verbose --watch",
        "test:cov": "cross-env NODE_ENV=test jest --coverage --passWithNoTests",
        "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
        "test:all": "npm run test && npm run test:e2e",
        "lint:fix": "tslint -c tslint.json --project ./tsconfig.json --fix",
        "lint:sa": "tslint --type-check --project tsconfig.json -c tslintsa.json 'src/**/*.ts' --exclude 'src/**/*.spec.ts'",
        "lint": "tslint -c tslint.json --project ./tsconfig.json",
        "exportdb": "node dist/data-export.js",
        "inst": "node dist/install.js",
