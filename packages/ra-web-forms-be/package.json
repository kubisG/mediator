{
    "name": "ra-web-forms-be",
    "version": "1.1.0",
    "description": "ra-web-forms-be",
    "author": "",
    "license": "MIT",
    "scripts": {
        "build": "rm -rf dist && cross-env NODE_ENV=production tsc",
        "bundle": "npm run build",
        "clean": "rimraf ./src/**/*.js ./src/**/*.js.map ./src/**/*.d.ts",
        "format": "prettier --write \"**/*.ts\"",
        "start": "ts-node -r tsconfig-paths/register src/main.ts",
        "start:dev": "tsc-watch --onSuccess \"node dist/main.js\"",
        "start:linux": "nodemon --config nodemon-linux.json",
        "start:forms": "nodemon --config nodemon-mediator.json",
        "prestart:prod": "rm -rf dist && tsc",
        "start:prod": "node dist/main.js",
        "start:hmr": "node dist/server",
        "test": "npm run clean && jest --passWithNoTests",
        "jest": "jest --showConfig",
        "test:cov": "npm run clean && jest --coverage --passWithNoTests",
        "test:e2e": "npm run clean && jest --config ./tests/jest-e2e.json",
        "webpack": "webpack --config webpack.config.js",
        "lint": "tslint -c tslint.json --project ./tsconfig.json",
        "lint:fix": "tslint -c tslint.json --project ./tsconfig.json --fix"
    },
    "dependencies": {
        "@nestjs/common": "6.5.3",
        "@nestjs/core": "6.5.3",
        "@nestjs/jwt": "6.1.1",
        "@nestjs/microservices": "6.5.3",
        "@nestjs/passport": "6.1.0",
        "@nestjs/platform-fastify": "6.5.3",
        "@nestjs/platform-socket.io": "6.5.3",
        "@nestjs/swagger": "3.1.0",
        "@nestjs/terminus": "6.5.2",
        "@nestjs/websockets": "6.5.3",
        "@godaddy/terminus": "4.2.0",
        "socket.io-redis": "5.2.0",
        "@ra/web-auth-be": "1.1.0",
        "@ra/web-core-be": "^1.1.0",
        "@ra/web-ee-router": "^1.1.0",
        "@ra/web-env-be": "^1.1.0",
        "@ra/web-queue": "^1.1.0",
        "@ra/web-shared-be": "^1.1.0",
        "cross-env": "5.2.0",
        "devextreme-angular": "19.1.4-build-19151-2014",
        "dotenv": "6.0.0",
        "fastify": "2.7.1",
        "fastify-formbody": "2.0.0",
        "golden-layout": "1.5.9",
        "light-mapper": "1.1.6",
        "pg-listen": "^1.4.0",
        "reflect-metadata": "0.1.13",
        "rxjs": "6.5.1",
        "soap": "^0.27.1",
        "typescript": "3.2.4",
        "pg": "7.9.0",
        "pkginfo": "0.4.1",
        "morgan": "1.9.1"
    },
    "devDependencies": {
        "@nestjs/testing": "6.5.3",
        "@types/express": "4.0.39",
        "@types/jest": "^24.0.17",
        "@types/node": "9.6.49",
        "@types/supertest": "2.0.4",
        "tsc-watch": "2.2.1",
        "jest": "24.8.0",
        "nodemon": "1.19.0",
        "prettier": "1.11.1",
        "rimraf": "2.6.1",
        "supertest": "3.0.0",
        "ts-jest": "21.2.4",
        "ts-loader": "4.1.0",
        "ts-node": "5.0.1",
        "tsconfig-paths": "3.1.1",
        "tslint": "^5.18.0"
    }
}
