{
    "production": false,
    "logging": {
        "provider": 0,
        "remote": true,
        "db": true
    },
    "app": {
        "port": 3000,
        "version": "1.1.0"
    },
    "cors": {
        "origin": [
            "http://localhost:4200"
        ]
    },
    "auth": {
        "expiresIn": 96000,
        "secretKey": "secretKey",
        "sessionStore": 0
    },
    "queue": {
        "type": "nats",
        "prefix": "comp_"
    },
    "verifyService": {
        "provider": "db"
    },
    "mailer": {
        "type": "smtp"
    },
    "worker": {
        "resending": 10
    }
}
