        "@ra/web-shared-be": "^1.1.0",
        "cross-env": "5.2.0",
        "devextreme-angular": "19.1.4-build-19151-2014",
        "dotenv": "6.0.0",
        "fastify": "2.7.1",
        "fastify-formbody": "2.0.0",
