{
    "name": "ra-web-monitor-be",
    "version": "1.1.0",
    "description": "ra-web-monitor-be",
    "author": "",
    "license": "MIT",
    "scripts": {
        "clean": "rimraf ./src/**/*.js ./src/**/*.js.map ./src/**/*.d.ts",
        "format": "prettier --write \"**/*.ts\"",
        "start": "ts-node -r tsconfig-paths/register src/main.ts",
        "start:dev": "npm run clean && tsc-watch --onSuccess \"node dist/main.js\"",
        "start:nocors": "npm run clean && cross-env NODE_ENV=devnocors nodemon",
        "start:monitor": "nodemon --config nodemon-mediator.json",
        "build": "rm -rf dist && cross-env NODE_ENV=production tsc",
        "bundle": "npm run build",
        "build:dev": "rm -rf dist && tsc",
        "prestart:prod": "npm run build",
        "start:prod": "node dist/main.js",
        "start:hmr": "node dist/server",
        "test": "npm run clean && jest --passWithNoTests",
        "test:cov": "npm run clean && jest --coverage --passWithNoTests",
        "test:e2e": "npm run clean && jest --config ./tests/jest-e2e.json",
        "webpack": "webpack --config webpack.config.js",
        "lint": "tslint -c tslint.json --project ./tsconfig.json",
        "lint:fix": "tslint -c tslint.json --project ./tsconfig.json --fix"
    },
    "dependencies": {
